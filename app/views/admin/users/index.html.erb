<div class="container">
  <h2>会員一覧</h2><br />

  <div class="row">
    <div class="col-md-8 offset-md-2">
　    <table class="table table-hover">
	    　<tbody>
	    　  <% @users.each do |user| %>
	    　		<tr>
	    　			<td><%= image_tag user.get_profile_image(38,38), class: "rounded-circle" %></td>
	    　			<td>
	    　				<%= link_to user.name, admin_user_path(user.id) %>
	    　			</td>
	    　			<td><%= user.email %></td>
	    　		</tr>
	    　	<% end %>
	    　</tbody>
　    </table>
　  </div>
　</div>
</div>
